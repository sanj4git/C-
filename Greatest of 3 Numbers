#include <iostream>
using namespace std;

int main(){

    cout<<"Welcome to C++ \n";
    int a,b,c;
    cin>>a>>b>>c;

    if(a>b && a>c){
        cout<<"The Greatest Number is "<<a;

    }

    else if(b > c && b > a){
        cout<<"The Greatest Number is "<<b;
    }

    else{
        cout<<"The Greatest Number is "<<c;
    }

}
