#include <iostream>
using namespace std;

int main(){


    int n1, n2;
    char op;

    cout<<"Enter two Numbers ";
    cin>>n1>>n2;

    cout<<"Enter the operation ";
    cin>>op;

    switch(op){

    case '+':
        cout<<n1 + n2;
        break;

    case '-':
        cout<<n1 - n2;
        break;

    case '*':
        cout<<n1 * n2;
        break;

    case '/':
        cout<<n1 / n2;
        break;

    default:
        cout<<"Enter proper operator"<<endl;
        cout<<"Thank You!";
    }

}
